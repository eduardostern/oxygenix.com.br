          "_customHTML": "<section data-bs-version=\"5.1\" class=\"header2\" group=\"Header\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"10\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"10\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\">\n        <input type=\"checkbox\" title=\"Text\" name=\"showText\">\n        <header>Images</header>\n        <input type=\"checkbox\" title=\"Image 1\" name=\"showImageOne\">\n        <input type=\"image\" title=\"Image 1\" name=\"imageOne\" value=\"@PROJECT_PATH@/assets/images/1.jpg\" condition=\"showImageOne\">\n        <input type=\"checkbox\" title=\"Image 2\" name=\"showImageTwo\">\n        <input type=\"image\" title=\"Image 2\" name=\"imageTwo\" value=\"@PROJECT_PATH@/assets/images/camara-hiperbaricafotohome-600x400.jpg\" condition=\"showImageTwo\">\n        <input type=\"checkbox\" title=\"Image 3\" name=\"showImageThree\">\n        <input type=\"image\" title=\"Image 3\" name=\"imageThree\" value=\"@PROJECT_PATH@/assets/images/3.jpg\" condition=\"showImageThree\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Image\" value=\"@PROJECT_PATH@/assets/images/untitled-3-2000x1261.png\" selected>\n            <input type=\"color\" title=\"Color\" value=\"#dcc8bd\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#4b5b5b\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.3\" condition=\"overlay && bg.type !==